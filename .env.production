# Configurações do Ambiente
NODE_ENV=production
PORT=3000

# Configurações do Banco de Dados
DB_HOST=${RAILWAY_DB_HOST}
DB_PORT=${RAILWAY_DB_PORT}
DB_USERNAME=${RAILWAY_DB_USERNAME}
DB_PASSWORD=${RAILWAY_DB_PASSWORD}
DB_DATABASE=${RAILWAY_DB_DATABASE}

# Configurações de Segurança
JWT_SECRET=seu-secret-jwt-muito-seguro-producao
JWT_EXPIRATION=24h

# Configurações de CORS
CORS_ORIGIN=https://seu-dominio-frontend.com

# Configurações de Rate Limit
RATE_LIMIT_WINDOW_MS=900000  # 15 minutos
RATE_LIMIT_MAX=100